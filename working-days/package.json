{
  "name": "n8n-nodes-working-days",
  "version": "0.1.0",
  "description": "Custom n8n node for working days calculations with German public holidays",
  "license": "MIT",
  "homepage": "https://github.com/jroehl/n8n-nodes-working-days",
  "repository": {
    "type": "git",
    "url": "https://github.com/jroehl/n8n-nodes-working-days.git"
  },
  "bugs": {
    "url": "https://github.com/jroehl/n8n-nodes-working-days/issues"
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "working-days",
    "holidays",
    "business-days",
    "germany",
    "calendar"
  ],
  "author": "jroehl",
  "main": "dist/nodes/WorkingDays.node.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepack": "npm test && npm run build",
    "pack": "npm pack",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo \"No linter configured\"",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "working-days.svg"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/WorkingDays.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.110.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@types/luxon": "^3.7.1",
    "luxon": "^3.7.2"
  }
}
